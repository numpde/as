@article{BayatiKimSaberi2010,
	Abstract = {We present a nearly-linear time algorithm for counting and randomly generating simple graphs with a given degree sequence in a certain range. For degree sequence (d                                          i                )                  i=1                                      n                                   with maximum degree d                        max{\thinspace}=O(m                        1/4−$\tau$                        ), our algorithm generates almost uniform random graphs with that degree sequence in time O(md                        max{\thinspace}) where                                                                           {\$}m={\backslash}frac{\{}1{\}}{\{}2{\}}{\backslash}sum{\_}{\{}i{\}}d{\_}{\{}i{\}}{\$}                 is the number of edges in the graph and $\tau$ is any positive constant. The fastest known algorithm for uniform generation of these graphs (McKay and Wormald in J. Algorithms 11(1):52--67, 1990) has a running time of O(m                        2                        d                                          max{\thinspace}                  2                ). Our method also gives an independent proof of McKay's estimate (McKay in Ars Combinatoria A 19:15--25, 1985) for the number of such graphs.},
